for i in 16 24 32 48 64 72 144 192 256 294 512
do 
    convert -transparent white -scale ${i}x${i} drawing-final.svg "${i}x${i}.png" ;
    convert -transparent white -scale ${i}x${i} drawing-round.svg "${i}x${i}-round.png" ;
done
